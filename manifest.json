{
  "manifest_version": 2,

  "name": "Wimax Signal Strength",
  "description": "Shows the current signal strength of Wimax Device",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
	"http://192.168.1.1/",
	"<all_urls>"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
}
}
